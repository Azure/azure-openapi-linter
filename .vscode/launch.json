{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Node Attach",
            "port": 9229
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run tests",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "args": [
                "./src/typescript/azure-openapi-validator/tests/*.js",
                "--colors"
            ],
            "outFiles": [],
            "protocol": "inspector",
            "preLaunchTask": "build/typescript"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run tests currently",
            "cwd": "${workspaceRoot}",
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "TS_NODE_IGNORE": "false"
            },
            "program": "${workspaceRoot}/node_modules/mocha/bin/mocha",
            "args": [
                "-r",
                "ts-node/register",
                "--timeout",
                "999999",
                "--colors",
                "${file}"
            ],
            "outFiles": [],
            "protocol": "inspector"
        },
        {
            "type": "node",
            "protocol": "inspector",
            "request": "launch",
            "name": "run bootstrapper",
            "program": "${workspaceRoot}/src/index.js",
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "build"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}